{
  "name": "number-words",
  "version": "1.0.0-alpha.1",
  "description": "Convert a number into words",
  "license": "MIT",
  "repository": "andreruffert/number-words",
  "author": "Andr√© Ruffert",
  "main": "src/index.js",
  "scripts": {
    "test": "xo && tape test.js | tap-spec"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "xo": {
    "envs": [
      "node",
      "browser"
    ],
    "space": true,
    "esnext": true,
    "rules": {
      "brace-style": [
        2,
        "stroustrup"
      ],
      "babel/arrow-parens": 0,
      "babel/object-curly-spacing": 0
    }
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.2",
    "xo": "^0.11.2"
  }
}
